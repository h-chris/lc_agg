<header>
  <nav>
    <div class="navbar">
      <ul>
        <li id="nav-logo">
          <a href="/">
            <span id="logo_top">CS50x</span><br/>
            <span id="logo_bottom">STL Edition</span>
          </a>
        </li>
        <li id="nav-latest"><%= link_to "Latest", root_url %></li>
        <li id="nav-reddit"><%= link_to "reddit", reddit_path %></li>
        <li id="nav-youtube"><%= link_to "LaunchCode TV", youtube_path %></li>
        <li id="nav-twitter"><%= link_to "Twitter", twitter_path %></li>
        <span id="username">
          <% if current_user %>
              Logged In as <%= session[:username] %>
              (<span id="unread"><%= link_to "Unread", unread_path %></span>) | 
              <%= link_to "Log Out", reddit_logout_path %><br/>
          <% else %>
              <%= link_to "reddit Log In", reddit_login_path %>
          <% end %>
        </span>
      </ul>
    </div>
  </nav>
</header>
<%= render 'layouts/r_menu' %>
