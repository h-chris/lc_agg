<header>
  <nav>
    <div class="navbar">
      <ul>
        <li id="nav-logo">
          <a href="/">
            <span id="logo_top">This is</span><br/>
            <span id="logo_bottom">CS50x</span>
          </a>
        </li>
        <li id="nav-latest"><%= link_to "Latest", root_url %></li>
        <li id="nav-reddit"><%= link_to "reddit", reddit_path %></li>
        <li id="nav-twitter"><%= link_to "Twitter", twitter_path %></li>
        <li id="nav-youtube"><%= link_to "YouTube", youtube_path %></li>
        <li id="nav-user">
          <% if current_user %>
            <%= link_to "Log Out", reddit_logout_path %>
          <% else %>
            <%= form_tag sessions_path do %>
              <%= text_field_tag :username, nil, placeholder: "username" %>
              <%= password_field_tag :password, nil, placeholder: "password" %>
              <%= submit_tag "Log In" %>
            <% end %>
          <% end %>
        </li>
      </ul>
    </div>
  </nav>
</header>
